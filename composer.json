{
    "name": "somnambulist/collection",
    "type": "library",
    "description": "Collection implementation with no dependencies.",
    "keywords": [
        "domain",
        "collection",
        "set"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5",
        "symfony/var-dumper": "~5.4",
        "victorjonsson/markdowndocs": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Somnambulist\\Components\\Collection\\": "src/"
        }
